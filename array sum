#include <bits/stdc++.h>
using namespace std;
int main()
{
   int n, i, sum=0;
   cout<<"input size of array";
   cin>>n;
   int arr[n];
   cout<<"Input elements of array:";
   for(i=0; i<n; i++)
   cin>>arr[i];
   for(i=0; i<n; i++)
   sum = sum+arr[i];
   cout<<"\nSum of all array elements = "<<sum;
   cout<<endl;
   return 0;
}
